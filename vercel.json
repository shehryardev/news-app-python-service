{
  "version": 2,
  "builds": [
    {
      "src": "api.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api.py"
    }
  ],
  "functions": {
    "api.py": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "env": {
    "PYTHONPATH": "."
  }
}
